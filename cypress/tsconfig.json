{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "DOM"],
      "types": ["cypress", "node", "@testing-library/cypress"],
      "isolatedModules": false,
      "noEmit": true,
      "composite": true,
      "jsx": "react-jsx",
      "strict": true,
      "allowImportingTsExtensions": true,
      "rootDir": "../"
    },
    
    "include": ["**/*.tsx", "**/*.ts", "**/*.js", "**/*.jsx","**/*.json",  "../**/*.ts", "../**/*.tsx", "../**/*.js"]
  }
  